{
  "name": "ibmiotf",
  "version": "0.2.15",
  "description": "A library for developing device and application clients for IBM Internet of Things Foundation",
  "main": "dist/iotf-client.js",
  "dependencies": {
    "axios": "^0.5.4",
    "bluebird": "^2.9.34",
